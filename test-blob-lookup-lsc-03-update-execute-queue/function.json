{
 "bindings": [
  {
   "name": "inUpdateExecuteQueue",
   "type": "queueTrigger",
   "direction": "in",
   "queueName": "lookup-lsc-update-execute-queue",
   "connection": "AZURE_STORAGE_CONNECTION_STRING"
  },
  {
   "name": "inoutRawDataBlob",
   "type": "blob",
   "direction": "inout",
   "path": "{queueTrigger.containerName}/{queueTrigger.blobName}",
   "connection": "AZURE_STORAGE_CONNECTION_STRING"
  },
  {
   "name": "outDataDownloadBlob",
   "type": "blob",
   "direction": "out",
   "path": "{queueTrigger.containerName}/{queueTrigger.blobName}/{queueTrigger.startTime}.gzip",
   "connection": "AZURE_STORAGE_CONNECTION_STRING"
  },
  {
   "name": "outLookupTable",
   "type": "blob",
   "direction": "out",
   "tableName": "blobaccess",
   "partitionKey": "{container}_{blob}",
   "rowKey": "lookup",
   "connection": "AZURE_STORAGE_CONNECTION_STRING"
  }
 ],
 "disabled": false
}