{
 "bindings": [
  {
   "name": "inUpdateRequestQueue",
   "type": "queueTrigger",
   "direction": "in",
   "queueName": "lookup-lsc-update-request-queue",
   "connection": "AZURE_STORAGE_CONNECTION_STRING"
  },
  {
   "name": "inoutChangeTable",
   "type": "blob",
   "direction": "inout",
   "tableName": "blobaccess",
   "partitionKey": "{queueTrigger.containerName}/{queueTrigger.blobName}",
   "rowKey": "change",
   "connection": "AZURE_STORAGE_CONNECTION_STRING"
  },
  {
   "name": "outUpdateExecuteQueue",
   "type": "queue",
   "direction": "out",
   "queueName": "lookup-lsc-update-execute-queue",
   "connection": "AZURE_STORAGE_CONNECTION_STRING"
  },
  {
   "name": "outRawDataBlob",
   "type": "blob",
   "direction": "out",
   "path": "{queueTrigger.containerName}/{queueTrigger.blobName}",
   "connection": "AZURE_STORAGE_CONNECTION_STRING"
  }
 ],
 "disabled": false
}