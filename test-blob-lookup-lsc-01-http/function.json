{
 "bindings": [
  {
   "name": "req",
   "type": "httpTrigger",
   "direction": "in",
   "authLevel": "anonymous",
   "route": "api/test-blob/{container}/{*blob}"
  },
  {
   "name": "res",
   "type": "http",
   "direction": "out"
  },
  {
   "name": "inLookupBlob",
   "type": "blob",
   "direction": "inout",
   "path": "{container}/{blob}/_lookup.txt",
   "connection": "AZURE_STORAGE_CONNECTION_STRING"
  },
  {
   "name": "outUpdateRequestQueue",
   "type": "queue",
   "direction": "out",
   "queueName": "lookup-lsc-update-request-queue",
   "connection": "AZURE_STORAGE_CONNECTION_STRING"
  }
 ],
 "disabled": false
}