{
 "bindings": [
  {
   "name": "req",
   "type": "httpTrigger",
   "direction": "in",
   "authLevel": "anonymous",
   "route": "api/test-blob/{container}/{*blob}"
  },
  {
   "name": "res",
   "type": "http",
   "direction": "out"
  },
  {
   "name": "inLookupTable",
   "type": "table",
   "direction": "in",
   "tableName": "blobaccess",
   "partitionKey": "{container}/{blob}",
   "rowKey": "lookup",
   "connection": "AZURE_STORAGE_CONNECTION_STRING"
  },
  {
   "name": "outUpdateRequestQueue",
   "type": "queue",
   "direction": "out",
   "queueName": "lookup-lsc-update-request-queue",
   "connection": "AZURE_STORAGE_CONNECTION_STRING"
  }
 ],
 "disabled": false
}